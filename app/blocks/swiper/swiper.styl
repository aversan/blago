.swiper-container-wrapper {
	padding-left: 40px;
	padding-right: 40px;
	position: relative;
}

.swiper-container--logo .swiper-slide {
	text-align: center;
	display: -webkit-box;
	display: -ms-flexbox;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: center;
	-ms-flex-pack: center;
	-webkit-justify-content: center;
	justify-content: center;
	-webkit-box-align: center;
	-ms-flex-align: center;
	-webkit-align-items: center;
	align-items: center;
}

.swiper-container--logo .swiper-slide img {
	max-width: 100%;
	height: auto;
}

.swiper-container--logo a img {
	transition-duration: 0.3s;
	transition-timing-function: ease-out;
	transition-property: opacity;
	opacity .9;
}

.swiper-container--logo a:hover img {
	opacity 1;
}

.swiper-container--thumbs {
	width: 494px;
	margin-left: auto;
	margin-right: auto;
	margin-bottom: 24px;
}

.swiper-container--thumbs .swiper-slide {
	background-size: cover;
	background-position: center;
	width: 56px;
	height: 56px;
	border-radius: 100px;
	margin: 7px 0;
	position: relative;
	cursor: pointer;
	opacity: .5;
}

.swiper-container--thumbs .swiper-slide:hover {
	opacity: 1;
}

.swiper-container--thumbs .swiper-slide-active {
	cursor: default;
	opacity: 1;
}

.swiper-container--thumbs .swiper-slide-active:after {
	content: '';
	display: block;
	position: absolute;
	background: #faf4e4;
	border: 2px solid #396d36;
	left: -7px;
	right: -7px;
	top: -7px;
	bottom: -7px;
	border-radius: 100px;
	z-index: -1;
}

.swiper-container--recept {
	width: 100%;
	margin-left: auto;
	margin-right: auto;
}

.swiper-container-wrapper-note {
