.promo__header-line
	height 5px
	display block
	margin-left auto
	margin-right auto
	max-width 236px
	background url('data:image/png;base64,') 0 0 no-repeat
	overflow hidden
	margin-top 1.5rem
	margin-bottom 1.5rem
	width 60%

	.promo__title + &
		margin-top -.75rem

.promo__header
	margin-bottom 2rem

.promo__title
	margin-bottom 1.5rem

	&.h1
		margin-bottom 1.5rem

	&:last-child
		margin-bottom 0 !important

.promo__title
.promo__subtitle
	color #3d6d31 !important

.promo__subtitle
	font-family 'latoitalic', 'Trebuchet MS', Arial, sans-serif
	font-size 1.5625rem
	font-style normal
	font-weight normal
	line-height 1.2

.promo__footer
	margin-top 1.5rem

	.promo__footer-line
		margin-top 1.5rem

	.icon-btn + .icon-btn
		margin-left 15px


.promo__box-outline
	border 2px solid alpha(#75371c, .4)
	padding 20px
	border-radius 5px

.promo__box
	background-color #e9d8ad
	border-radius 5px

	&.uk-overlay
		display block
		overflow visible
		text-align center

		.uk-overlay-panel
			text-align left
			padding 40px 50px
			z-index 3

.promo__box--wide
	border-radius none

.promo__logo-wrapper
.promo__label-wrapper
	height 75px
	line-height 75px
	margin-bottom 40px
	padding-top 3px

.promo__logo-wrapper
	text-align center

.promo__logo
	display inline-block
	max-heght 75px

.promo__label-wrapper
	text-align left
	white-space nowrap

	.label
		display inline-block
		vertical-align middle

	.label + .label
		margin-left 15px

.promo__text
	color $text-base

	h1
	h2
	h3
	h4
	h5
		line-height 1.5
		margin-bottom .75rem

.promo__text--primary
	color $text-primary

.promo--main
	.promo__col-left .promo__text
		font-family 'latobold', 'Trebuchet MS', Arial, sans-serif

.promo__header--main-index
	padding-top 95px
	padding-bottom 95px
	text-align center
	max-width 420px
	margin-left auto
	margin-right auto
	position relative

.promo--main--index
	.promo__header
		padding-top 95px
		padding-bottom 95px
		text-align center
		max-width 420px
		margin-left auto
		margin-right auto
		position relative
		z-index 2111

	.promo__box-image
		margin-top -95px
		margin-bottom 50px

	.promo__col-left
		padding-right 120px

	.promo__col-right
		padding-left 120px

	.promo__box
		background-color transparent

		& > .uk-overlay-panel
			background-color #e9d8ad
			border-radius 5px

		.promo__box-image
			position relative
			z-index 4

		.decor__desc
			z-index 3

	.container-fluid
		position relative

.promo--main--about
	.promo__box-image
		margin-top 40px
		position relative
		left 23%

.promo--recept
	min-height 1200px

	.promo__header
		text-align center

	.promo__image
		display block
		margin-left auto
		margin-right auto
		margin-bottom -10px

	.promo__note
		margin-top -85px
		left -30px
		width 721px
		margin-left auto
		margin-right auto
		padding 210px 65px 70px 95px
		margin-bottom 132px
		position relative

		&:before
			content ''
			display block
			background-color #f7e1cb
			position absolute
			left 50px
			top 155px
			right 15px
			bottom 30px
			z-index -1

	.promo__col-left
		padding-top 22px
		padding-right 10px

	.promo__col-right
		padding-left 10px

	.promo__box-outline
		margin-right -22px

	ol.decimal
		margin-left -1.75em

	.promo__collapse
		padding-left 28px
		padding-right 22px
		height 40rem
		min-height 40rem
		overflow hidden
		position relative
		z-index 1

		.promo__header2-primary
			margin-left -28px
			margin-right -22px

		&.in
			height auto

		&:after
			content ''
			display block
			position absolute
			left 0
			right 0
			bottom 0
			height 90px
			overflow hidden
			background url('data:image/png;base64,') 0 0 repeat-x

		&.in
			&:after
				display none

	.promo__toggle
		.button__text
			display none

		.button__text:first-child
			display inline

	.promo__collapse.in + .promo__footer
		.promo__toggle
			.button__text
				display inline

			.button__text:first-child
				display none

	.collapse.promo__collapse
		display block

	.promo__table
		th:last-child
		td:last-child
			white-space nowrap
			vertical-align bottom

	.promo__header2
		font-size 16px
		line-height 1.5
		font-family 'latobold', 'Trebuchet MS', Arial, sans-serif
		font-weight 700
		color #3d2405
		display table
		vertical-align bottom
		position relative
		padding-bottom 8px
		margin-top 2rem
		margin-bottom 1.5rem

		&:before
			content ''
			display block
			height 5px
			background url('data:image/png;base64,') 0 0 repeat-x
			overflow hidden
			position absolute
			left 0
			right 0
			bottom 0

		.icon
			margin-right 5px

		.promo__header2-primary
			width 10000px
			display table-cell
			text-align left

		.promo__header2-secondary
			display table-cell
			text-align right
			white-space nowrap


.promo--recept--alt
	min-height auto

	.promo__note
		margin-bottom 0
